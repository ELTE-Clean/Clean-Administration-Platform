{
  "name": "web-app",
  "private": true,
  "scripts": {
    "dev": "npm run sass-dev & next dev",
    "build": "next build",
    "start": "next start",
    "prod": "npm run build && next start -p 80",
    "lint": "next lint",
    "sass-dev": "sass --watch --update --style=expanded styles/App.scss:styles/App.css",
    "sass-prod": "sass --no-source-map --style=compressed styles/App.scss:styles/App.css"
  },
  "dependencies": {
    "next": "^12.0.10",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "^1.49.7"
  },
  "devDependencies": {
    "@types/node": "16.11.10",
    "@types/react": "17.0.37",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.4",
    "typescript": "4.5.2"
  }
}
